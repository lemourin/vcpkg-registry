{
  "name": "coro-http",
  "version": "0.1.0",
  "port-version": 31,
  "dependencies": [
    "boost-regex",
    {
      "name": "curl",
      "default-features": false,
      "features": [
        "c-ares",
        "http2",
        "http3",
        "ssl"
      ]
    },
    "libevent",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "stacktrace": {
      "description": "Enable stacktraces in exceptions.",
      "dependencies": [
        "boost-stacktrace",
        {
          "name": "libbacktrace",
          "platform": "!windows | mingw"
        }
      ]
    }
  }
}
